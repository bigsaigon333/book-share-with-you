<form action="./search" method="GET" id="form-search">
	<label for="form_search__input">Search</label>
	<input type="text" name="title" id="form-search__input" value="<%= title %>" />
	<input type="submit" value="검색">
</form>

<% if (locals.books != null ) { %>
<% books.forEach(book => { %> 
	<div>

		<h3> <%= book.title.replace(/(<\/b>)|(<b>)/gi, "") %> </h3>
		<img src="<%= book.image%>" alt="book image" width="82" height="109">
		<span><%= book.author  %> </span>
		<span><%= book.publisher  %> </span>
		<span><%= book.pubdate  %> </span>
	</div>
	
	
	<% })} %>
